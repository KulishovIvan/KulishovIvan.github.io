<script>
var mapOptions = {
   center: [54.53, 36.26],
   zoom: 10
   }
var map = new L.map('map', mapOptions);

var marker = new L.Marker(new L.LatLng(54.53, 36.26));
map.addLayer(marker);
var layer = new L.TileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png');

map.addLayer(layer);
</script>

